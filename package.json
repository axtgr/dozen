{
  "name": "dozen",
  "version": "0.10.0",
  "description": "Load config from a dozen sources, normalize and validate with Zod",
  "author": "Alex Schneider <me@schneider.ax>",
  "license": "ISC",
  "repository": "https://github.com/axtgr/dozen",
  "keywords": [
    "config",
    "configuration",
    "env",
    "environment",
    "variables",
    "dotenv",
    "argv",
    "validate",
    "validation",
    "schema",
    "zod"
  ],
  "private": true,
  "packageManager": "bun@1.2.4",
  "engines": {
    "node": ">=23.6.0"
  },
  "type": "module",
  "main": "./src/presets/node.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/presets/node.ts",
    "./base": "./src/index.ts",
    "./plugins/*": "./src/plugins/*.ts"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@standard-schema/spec": "^1.0.0",
    "chokidar": "^4.0.3",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.4.7",
    "text-case": "^1.0.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.10",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2"
  }
}
