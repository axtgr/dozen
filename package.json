{
  "name": "dozen",
  "version": "1.2.0",
  "description": "Load config from a dozen sources, transform and validate with Zod, Valibot or ArkType",
  "author": "Alex Schneider <me@schneider.ax>",
  "license": "ISC",
  "repository": "https://github.com/axtgr/dozen",
  "keywords": [
    "config",
    "configuration",
    "env",
    "environment",
    "variables",
    "dotenv",
    "argv",
    "validate",
    "validation",
    "schema",
    "zod"
  ],
  "private": true,
  "packageManager": "bun@1.2.4",
  "engines": {
    "node": ">=23.6.0",
    "bun": ">=1.2.4"
  },
  "type": "module",
  "main": "./src/presets/node.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/presets/node.ts",
    "./base": "./src/index.ts",
    "./plugins/*": "./src/plugins/*.ts"
  },
  "scripts": {
    "test": "bun test"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@standard-schema/spec": "^1.0.0",
    "chokidar": "^4.0.3",
    "json5": "^2.2.3",
    "text-case": "^1.0.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.2.8",
    "@types/node": "^22.13.10",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2"
  }
}
